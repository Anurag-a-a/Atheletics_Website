    <br><br>
        <a class="go_home_link" href="/user/landingPage">Go to Home Page</a><br>
        <h1 class = "textCenter">Location list</h1>
<div class="  gyms gym-detail">
    {{#each locations}}
    {{!-- <div class="detailscont cls-detail"> --}}
    

    <div class=" form-cont">
        <h2 class="gym-name textCenter">
        {{  this.branchName  }}
    </h2>
        <p>
            Website: 
            <a href="">
                {{  this.website    }}
            </a>
        </p>
        <p>
            Address: 
            
            {{#if this.address.streetName.length}}
                {{  this.address.streetName  }}
            {{else}}
                "N/A"
            {{/if}}
            {{#if this.address.city.length}}
                {{  this.address.city  }}
            {{else}}
                "N/A"
            {{/if}}
            {{#if this.address.state.length}}
                {{  this.address.state  }}
            {{else}}
                "N/A"
            {{/if}}
            {{#if this.address.zip.length}}
                {{  this.address.zip  }}
            {{else}}
                "N/A"
            {{/if}}
        </p>
        <p>
            Phone: 
            {{  this.phoneNumber    }}
        </p>
        <p>
            Email: {{   this.email }}
        </p>
        <p>
            Max Capacity : {{ this.maxCapacity    }}
        </p>
    </div>
        {{/each}}
{{!-- </div> --}}
</div>
