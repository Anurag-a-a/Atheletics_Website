<h1>Create Event</h1>
	<form id="classCreation" method="POST" action="gym/addClass">
		<label for="className">Class Name:</label>
		<input type="text" id="className" name="className" required><br><br>

		<label for="date1">Date:</label>
		<input type="date" id="date" name="date" required><br><br>

		<label for="timings1">Timings:</label>
		<input type="text" id="timings" name="timings" required><br><br>

		<label for="instructor">Instructor:</label>
		<input type="text" id="instructor" name="instructor" required><br><br>

		<label for="description">Description:</label>
		<textarea id="description" name="description" required></textarea><br><br>

		<input type="submit" value="addClass">Create Class</button>
	</form>